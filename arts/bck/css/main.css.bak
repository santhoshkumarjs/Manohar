@import url(http://fonts.googleapis.com/css?family=Open+Sans:400,800,300,600,700);
@import url(http://fonts.googleapis.com/css?family=Abel);

body {
  font-family: "Open Sans", Arial, sans-serif;
  background:#f3f3f3;
  position: relative;
  font-weight:400px;
}

ul li {
  list-style: none;
}

a:hover {
outline: none;
text-decoration:none;
}

a:focus {
  outline:none;
  outline-offset: 0;
}

a {
  -webkit-transition: 300ms;
  -moz-transition: 300ms;
    -o-transition: 300ms;
    transition: 300ms;
}

h1, h2, h3, h4, h5, h6 {
  font-family: "Open Sans", Arial, sans-serif;
}

.btn:hover, 
.btn:focus{
  outline: none;
  box-shadow: none;
}

.navbar-toggle {
  background-color: #fff;
}
.navbar-toggle {
    position: relative;
    /*float: right;*/
       
    margin-right: 60px;
    margin-bottom: 8px;
    background-color: transparent;
    background-image: none;
    border: 1px solid transparent;
    border-radius: none !important;
}


/*************************
*******Header CSS******
**************************/

.header_top {
  background: none repeat scroll 0 0 #fff;
}

.header-bottom{
	margin: 0px !important;
}
.left-half{
	float: left;
	width: 45%;
	padding:5px;
	
}
.center-half{width: 5%;border-left:1px solid #bbb;float:left;margin-top: 10px;height: 62px;}
.right-half{
	float: right;
	width: 45%;
