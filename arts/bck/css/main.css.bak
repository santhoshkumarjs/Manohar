}
.right-half h5{margin-bottom:5px;}
.left-half h5{margin-bottom:5px;}
.right-half,
.left-half p{font-size:12px;}
.header-bottom{
	position: absolute;
	z-index: 100;
	width: 100%;
	background: transparent !important;
}

.box-shadow {
	box-shadow: 0px 3px 4px #dcdcdc;
	-webkit-box-shadow: 0px 3px 4px #dcdcdc;
	-moz-box-shadow: 0px 3px 4px #dcdcdc;
}

/*.search_box{
		width: 100%;
}*/

.contactinfo ul li:first-child{
    margin-left: -15px;
}

